{
  "name": "@mobylab/moby",
  "version": "0.1.3",
  "description": "Web components set",
  "private": false,
  "keywords": "vue, vue-components, mobylab",
  "author": "Mobydigital <contact@mobydigital.com> (https://www.mobydigital.com/)",
  "contributors": [
    "Ignacio Arguello <iarguello@mobydigital.com>"
  ],
  "license": "MIT",
  "repository": "yoelnacho/vue-components",
  "homepage": "https://www.mobydigital.com/",
  "files": [
    "dist/"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "component": "vue-cli-service build --target wc --name moby 'src/components/*.vue'"
  },
  "dependencies": {
    "vue": "^2.5.17"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.4",
    "@vue/cli-plugin-eslint": "^3.0.4",
    "@vue/cli-service": "^3.0.4",
    "@vue/eslint-config-standard": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0-0",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
